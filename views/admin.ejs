<!doctype html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Bootstrap demo</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-T3c6CoIi6uLrA9TneNEoa7RxnatzjcDSCmG1MXxSR1GAsXEV/Dwwykc2MPK8M2HN" crossorigin="anonymous">
  </head>
  <body>
    <h1>OGX AUTOS!</h1>
    <%- include("navbar/up") %>

    <p>Ini profile</p>
    <p>Anda Masuk Sebagai Admin <%= data.name %></p>

    <% car.forEach(res => {%>
      <div class="col-md-4">
          <div class="card mb-3" style="width: 18rem;">
              <img src="<%= res.carImage %>" class="card-img-top" alt="...">
              <div class="card-body">
                  <h5 class="card-title"><%= res.name %></h5>
                  <h6><%= res.CategoryId  %></h6>
                  <h6>(<%= res.formatPrice  %>)</h6>
                  <a href="/cars/<%= res.id %>/delete/<%= data.UserId %>" class="btn btn-danger">Delete</a>
              </div>
          </div>
      </div>
  <% })  %>
    
  </body>
</html>